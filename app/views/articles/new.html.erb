<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <div>
        <h2>
          <%= link_to "投稿一覧へ", articles_path %>
        </h2>
      </div>
      <div class="blog-new-form">
        <%= form_for(@article) do |f| %>
        <p>タイトル</p>
        <%= f.text_field :title %>
        <p>本文</p>
        <%= f.text_area :text %>
        <%= f.submit 'OK' %>
        <% end %>
      </div>
    </div>
  </div>
</div>