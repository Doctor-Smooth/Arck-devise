<div class="container">
  <div class="row">
    <div>
      <h2>
        <%= link_to "投稿一覧へ", blogs_path %>
      </h2>
    </div>
    <div class="blog-new-form">
      <%= form_for(@blog) do |f| %>
      <p>タイトル</p>
      <%= f.text_field :title %>
      <p>本文</p>
      <%= f.text_area :text %>
      <%= f.submit 'OK' %>
      <% end %>
    </div>
  </div>
</div>